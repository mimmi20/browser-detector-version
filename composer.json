{
    "name": "mimmi20/browser-detector-version",
    "description": "Version Class for the Browser Detector",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "version"
    ],
    "authors": [
        {
            "name": "Thomas MÃ¼ller",
            "homepage": "https://github.com/mimmi20",
            "role": "Developer"
        },
        {
            "name": "Contributors",
            "homepage": "https://github.com/mimmi20/browser-detector-version/graphs/contributors"
        }
    ],
    "homepage": "https://github.com/mimmi20/browser-detector-version",
    "support": {
        "issues": "https://github.com/mimmi20/browser-detector-version/issues",
        "source": "https://github.com/mimmi20/browser-detector-version"
    },
    "require": {
        "php": "^8.1.0",
        "ext-mbstring": "*",
        "psr/log": "^3.0.0"
    },
    "require-dev": {
        "ext-ctype": "*",
        "ext-dom": "*",
        "ext-simplexml": "*",
        "ext-tokenizer": "*",
        "ext-xml": "*",
        "ext-xmlwriter": "*",
        "infection/infection": "^0.27.9",
        "mimmi20/coding-standard": "^5.2.6",
        "nikic/php-parser": "^4.18.0",
        "phpstan/extension-installer": "^1.3.1",
        "phpstan/phpstan": "^1.10.50",
        "phpstan/phpstan-deprecation-rules": "^1.1.4",
        "phpstan/phpstan-phpunit": "^1.3.15",
        "phpunit/phpunit": "^10.5.5",
        "rector/rector": "^0.18.13"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "BrowserDetector\\Version\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BrowserDetectorTest\\Version\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "lock": false,
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "phpstan/extension-installer": true,
            "infection/extension-installer": true
        }
    }
}
