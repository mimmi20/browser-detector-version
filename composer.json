{
  "name": "mimmi20/browser-detector-version",
  "type": "library",
  "description": "Version Class for the Browser Detector",
  "keywords": [
    "version"
  ],
  "homepage": "https://github.com/mimmi20/version",
  "license": "MIT",
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    },
    {
      "name": "Contributors",
      "homepage": "https://github.com/mimmi20/version/graphs/contributors"
    }
