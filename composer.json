    "mimmi20/coding-standard": "^2.2.3",
    "pepakriz/phpstan-exception-rules": "^0.11.7",
    "phpstan/extension-installer": "^1.1.0",
    "phpstan/phpstan": "^0.12.90",
    "phpstan/phpstan-deprecation-rules": "^0.12.6",
    "phpstan/phpstan-phpunit": "^0.12.21",
    "phpunit/phpunit": "^9.5.6"
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "BrowserDetector\\Version\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BrowserDetectorTest\\Version\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "support": {
    "issues": "https://github.com/mimmi20/version/issues",
    "source": "https://github.com/mimmi20/version"
